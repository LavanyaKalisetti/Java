package com.nt.interfaces;

import java.util.ArrayList;
import java.util.LinkedHashSet;

public interface PlaylistMediaPlayer {
	void play();
    void pause();
    void stop();
    void volume(int volumeLevel);
    void track();
    void managePlaylist();
    void createPlaylist(String playListName,LinkedHashSet<Song> s);
	void mediaItems(String playListName, Song... song);
	void createPlaylist(String playListName, ArrayList<Song> songsList);
	
	
}
