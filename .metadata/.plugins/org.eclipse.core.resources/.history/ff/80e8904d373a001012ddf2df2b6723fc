package com.nt.pgms;

public class Palindrome {
	public static void main(String[] args) {
		String str="madam";
				int left=0,right=str.length()-1;
				while(left<right) {
					if(str.charAt(left)!=str.charAt(right)) {
						break;
					}else {
						left++;
						right--;
					}
				}
				if(left==right) {
					System.out.println("Plaindrome");
					}else {
						System.out.println("Not Palindrome");
					}
				}
}
